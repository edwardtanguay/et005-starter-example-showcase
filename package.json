{
  "name": "vite-vanilla-dark",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "run-p backend frontend test",
    "backend": "nodemon",
    "frontend": "vite",
    "buildBackend": "tsc -p ./cli/tsconfig.json",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "ce": "node distBackend/cli/ce.js",
    "test": "vitest"
  },
  "nodemonConfig": {
    "watch": [
      "cli"
    ],
    "ext": "ts",
    "exec": "npm run buildBackend"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.3",
    "@vitest/coverage-c8": "^0.32.2",
    "nodemon": "^2.0.22",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vitest": "^0.32.2"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5",
    "sass": "^1.62.1"
  }
}
